<script>
document.addEventListener('DOMContentLoaded', function() {
  // Elementos do carrossel
  const carousel = document.getElementById('detran-carousel');
  const slides = document.querySelectorAll('.detran-slide');
  const prevBtn = document.getElementById('detran-prev');
  const nextBtn = document.getElementById('detran-next');
  const indicators = document.getElementById('detran-indicators');
  const searchInput = document.getElementById('detran-search');
  
  // Configura????es
  const slidesToShow = {
    mobile: 2,    // Celulares
    sm: 3,        // Tablets pequenos
    md: 5,        // Tablets
    lg: 6         // Desktop
  };
  
  let currentPage = 0;
  let totalPages = 0;
  let slideWidth = 0;
  let slidesPerPage = getSlidesPerPage();
  
  // Inicializar carrossel
  function initCarousel() {
    slidesPerPage = getSlidesPerPage();
    totalPages = Math.ceil(slides.length / slidesPerPage);
    
    // Criar indicadores de p??gina
    indicators.innerHTML = '';
    for (let i = 0; i < totalPages; i++) {
      const dot = document.createElement('button');
      dot.classList.add('w-3', 'h-3', 'rounded-full', 'bg-gray-300');
      if (i === 0) dot.classList.add('bg-primary');
      dot.setAttribute('aria-label', `P??gina ${i+1} de ${totalPages}`);
      dot.dataset.page = i;
      
      dot.addEventListener('click', function() {
        goToPage(parseInt(this.dataset.page));
      
      indicators.appendChild(dot);
    }
    
    // Atualizar largura e posi????o
    updateCarousel();
  }
  
  // Obter n??mero de slides por p??gina baseado no tamanho da tela
  function getSlidesPerPage() {
    const width = window.innerWidth;
    if (width < 640) return slidesToShow.mobile;
    if (width < 768) return slidesToShow.sm;
    if (width < 1024) return slidesToShow.md;
    return slidesToShow.lg;
  }
  
  // Atualizar carrossel
  function updateCarousel() {
    // Calcular largura do slide baseado no container
    const carouselWidth = carousel.parentElement.clientWidth;
    slideWidth = carouselWidth / slidesPerPage;
    
    // Ajustar largura de cada slide
    slides.forEach(slide => {
      slide.style.width = `${slideWidth}px`;
    
    // Mover para a p??gina atual
    carousel.style.transform = `translateX(-${currentPage * slidesPerPage * slideWidth}px)`;
    
    // Atualizar indicadores
    const dots = indicators.querySelectorAll('button');
    dots.forEach((dot, i) => {
      if (i === currentPage) {
        dot.classList.add('bg-primary');
        dot.classList.remove('bg-gray-300');
      } else {
        dot.classList.remove('bg-primary');
        dot.classList.add('bg-gray-300');
      }
    
    // Gest??o dos bot??es de navega????o
    if (currentPage === 0) {
      prevBtn.classList.add('opacity-50', 'cursor-not-allowed');
} else {
      prevBtn.classList.remove('opacity-50', 'cursor-not-allowed');
    }
    
    if (currentPage === totalPages - 1) {
      nextBtn.classList.add('opacity-50', 'cursor-not-allowed');
} else {
      nextBtn.classList.remove('opacity-50', 'cursor-not-allowed');
    }
  }
  
  // Ir para a p??gina espec??fica
  function goToPage(page) {
    if (page < 0 || page >= totalPages) return;
    currentPage = page;
    updateCarousel();
  }
  
  // Navegar para a pr??xima p??gina
  function goToNextPage() {
    if (currentPage < totalPages - 1) {
      currentPage++;
      updateCarousel();
    }
  }
  
  // Navegar para a p??gina anterior
  function goToPrevPage() {
    if (currentPage > 0) {
      currentPage--;
      updateCarousel();
    }
  }
  
  // Fun????o de pesquisa de estados
  function searchDetrans(query) {
    const noResultsMsg = document.getElementById('no-results-message');
    
    if (!query) {
      // Se a busca estiver vazia, mostrar todos os slides
      slides.forEach(slide => {
        slide.style.display = '';
      noResultsMsg.classList.add('hidden');
      return;
    }
    
    // Filtrar slides baseado na busca
    const normalizedQuery = query.toLowerCase().normalize("NFD").replace(/[\u0300-\u036f]/g, "");
    let anyMatch = false;
    
    slides.forEach(slide => {
      const stateName = slide.querySelector('span').textContent;
      const normalizedStateName = stateName.toLowerCase().normalize("NFD").replace(/[\u0300-\u036f]/g, "");
      
      if (normalizedStateName.includes(normalizedQuery)) {
        slide.style.display = '';
        anyMatch = true;
} else {
        slide.style.display = 'none';
      }
    
    // Mostrar ou ocultar a mensagem de "nenhum resultado encontrado"
    if (!anyMatch) {
      // Mostrar mensagem de nenhum resultado
      noResultsMsg.classList.remove('hidden');
      
      // Ocultar controles de navega????o
      prevBtn.style.display = 'none';
      nextBtn.style.display = 'none';
      indicators.style.display = 'none';
} else {
      // Ocultar mensagem de nenhum resultado
      noResultsMsg.classList.add('hidden');
      
      // Mostrar controles de navega????o
      prevBtn.style.display = '';
      nextBtn.style.display = '';
      indicators.style.display = '';
      
      // Sempre voltar para a primeira p??gina ap??s uma busca
      currentPage = 0;
      updateCarousel();
    }
  }
  
  // Event listeners
  prevBtn.addEventListener('click', goToPrevPage);
  nextBtn.addEventListener('click', goToNextPage);
  
  // Busca com throttling
  let searchTimeout;
  searchInput.addEventListener('input', function() {
    clearTimeout(searchTimeout);
    searchTimeout = setTimeout(() => {
      searchDetrans(this.value);
    }, 300);
  
  // Lidar com o redimensionamento da janela
  window.addEventListener('resize', function() {
    const newSlidesPerPage = getSlidesPerPage();
    if (newSlidesPerPage !== slidesPerPage) {
      // Se o n??mero de slides por p??gina mudar, reinicializar o carrossel
    } else {
      // Sen??o, apenas atualizar a largura dos slides
      updateCarousel();
    }
  
<!-- Scripts -->
